<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.njwb.dao.RendDao" >
    <resultMap type="Rend" id="RendResult">
        <id property="id" column="id" />
        <result property="rend_address" column="rend_address" />
        <result property="rend_man" column="rend_man" />
        <result property="rend_start" column="rend_start" />
    </resultMap>

    <select id="queryAllRend" resultType="Rend" parameterType="String">
        select * from t_rend_info
    </select>
    <select id="searchRendById" resultType="Rend" parameterType="String" >
        select * from t_rend_info 
        <where>
			<if test="device_Id!= null">
				 rend_Id like CONCAT("%",#{rend_Id},'%')
			</if>
		</where>
		order by rend_Id
    </select>
    <delete id="deleteRend" parameterType="String">
        delete from t_rend_info  where rend_Id=#{rend_Id}
    </delete>
    <update id="modifyRendEnd" parameterType="String" >
        update t_rend_info set rend_end =#{rend_end} where rend_Id=#{rend_Id}
    </update>

</mapper>
